/*Write a program to print a table of 5*/
#include<stdio.h>
int main()
{
int i;
int table;
// input a table number
printf("Enter the table which you want to see :");
scanf("%d",&table);
// start counter loop from 1,increment by 1 in each iteration
for(int i=1;i<=10;i++){
    printf("%d X %d = %d\n",table,i,table*i);
}
return 0;
}
